{
  "name": "back-end",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "ts": "tsc",
    "ts:watch": "yarn ts --watch",
    "js": "NODE_ENV=production esbuild src/main/server.ts --bundle --minify --platform=node --target=node14 --outdir=dist",
    "js:watch": "yarn js --watch",
    "dev": "concurrently \"yarn js:watch\" \"nodemon dist/server.js\"",
    "server": "json-server src/db.json -p 8000 --watch"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.17",
    "eslint": "^8.0.1",
    "eslint-config-idiomatic": "^4.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.10.0",
    "jest": "^29.5.0",
    "json-server": "^0.17.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/mongodb": "^4.0.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "mongodb": "^5.3.0"
  }
}
